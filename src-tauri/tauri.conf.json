{
  "package": {
    "productName": "Tongfang Utility",
    "version": "4.0.8"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:3000",
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "withGlobalTauri": true
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.kirainmoe.tongfang.utility",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "Ami Technology",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "minimumSystemVersion": "",
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "entitlements": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": true,
      "endpoints": [
        "http://localhost:9000/api/utility/update/{{target}}/{{current_version}}",
        "https://utility-rinco.kirainmoe.com:30000/api/utility/update/{{target}}/{{current_version}}",
        "https://utility-eine.kirainmoe.com:1900/api/utility/update/{{target}}/{{current_version}}",
        "https://utility-akane.kirainmoe.com/api/utility/update/{{target}}/{{current_version}}"
      ],
      "dialog": false
    },
    "allowlist": {
      "fs": {
        "all": true
      },
      "window": {
        "all": true
      },
      "os": {
        "all": true
      },
      "path": {
        "all": true
      },
      "dialog": {
        "open": true
      },
      "shell": {
        "execute": true
      }
    },
    "windows": [
      {
        "title": "Tongfang Utility",
        "width": 900,
        "height": 700,
        "resizable": false,
        "fullscreen": false,
        "decorations": false
      }
    ],
    "security": {
      "csp": "default-src blob: data: filesystem: ws: wss: http: https: tauri: 'unsafe-eval' 'unsafe-inline' 'self' img-src: 'self'"
    }
  }
}
